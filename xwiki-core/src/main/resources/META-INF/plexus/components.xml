<component-set>
  <components>
    <component>
      <role>org.xwiki.velocity.VelocityManager</role>
      <role-hint>default</role-hint>
      <implementation>com.xpn.xwiki.render.DefaultVelocityManager</implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
      <requirements>
        <requirement>
          <role>org.xwiki.context.Execution</role>
        </requirement>
      </requirements>
    </component>
    <component>
      <role>org.xwiki.velocity.VelocityContextInitializer</role>
      <role-hint>default</role-hint>
      <implementation>com.xpn.xwiki.render.XWikiVelocityContextInitializer</implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
      <requirements>
        <requirement>
          <role>org.xwiki.context.Execution</role>
        </requirement>
      </requirements>
    </component>
    <component>
      <role>org.xwiki.rendering.DocumentManager</role>
      <role-hint>default</role-hint>
      <implementation>com.xpn.xwiki.doc.DefaultDocumentManager</implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
      <lifecycle-handler>xwiki</lifecycle-handler>
      <requirements>
        <requirement>
          <role>org.xwiki.context.Execution</role>
        </requirement>
      </requirements>
    </component>
    <!-- store components -->
    <component>
      <role>com.xpn.xwiki.store.XWikiStoreInterface</role>
      <implementation>com.xpn.xwiki.store.XWikiHibernateStore</implementation>
    </component>
    <component>
      <role>com.xpn.xwiki.store.XWikiVersioningStoreInterface</role>
      <implementation>com.xpn.xwiki.store.XWikiHibernateVersioningStore</implementation>
    </component>
    <component>
      <role>com.xpn.xwiki.store.XWikiAttachmentStoreInterface</role>
      <implementation>com.xpn.xwiki.store.XWikiHibernateAttachmentStore</implementation>
    </component>
    <component>
      <role>com.xpn.xwiki.store.AttachmentVersioningStore</role>
      <implementation>com.xpn.xwiki.store.hibernate.HibernateAttachmentVersioningStore</implementation>
    </component>
    <component>
      <role>com.xpn.xwiki.store.AttachmentVersioningStore</role>
      <role-hint>void</role-hint>
      <implementation>com.xpn.xwiki.store.VoidAttachmentVersioningStore</implementation>
    </component>
    <component>
      <role>com.xpn.xwiki.store.XWikiRecycleBinStoreInterface</role>
      <implementation>com.xpn.xwiki.store.XWikiHibernateRecycleBinStore</implementation>
    </component>
    <component>
      <role>com.xpn.xwiki.store.AttachmentRecycleBinStore</role>
      <implementation>com.xpn.xwiki.store.hibernate.HibernateAttachmentRecycleBinStore</implementation>
    </component>
  </components>
</component-set>
