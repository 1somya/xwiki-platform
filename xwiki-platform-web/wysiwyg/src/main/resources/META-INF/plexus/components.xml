<component-set>
  <components>
    <component>
      <role>com.xpn.xwiki.wysiwyg.server.converter.XHTMLConverter</role>
      <role-hint>xhtml</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.converter.internal.FakeXHTMLConverter
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>
    <component>
      <role>com.xpn.xwiki.wysiwyg.server.converter.XHTMLConverter</role>
      <role-hint>xwiki/1.0</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.converter.internal.RadeoxXWikiXHTMLConverter
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>
    <component>
      <role>com.xpn.xwiki.wysiwyg.server.converter.XHTMLConverter</role>
      <role-hint>xwiki/2.0</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.converter.internal.XWikiXHTMLConverter
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>
    <component>
      <role>com.xpn.xwiki.wysiwyg.server.converter.XHTMLConverter</role>
      <role-hint>events</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.converter.internal.EventsXHTMLConverter
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>

    <component>
      <role>com.xpn.xwiki.wysiwyg.server.cleaner.XHTMLCleaner</role>
      <role-hint>jtidy</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.cleaner.internal.JTidyXHTMLCleaner
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>
    <component>
      <role>com.xpn.xwiki.wysiwyg.server.cleaner.XHTMLCleaner</role>
      <role-hint>vnikic</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.cleaner.internal.VnikicXHTMLCleaner
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>

    <component>
      <role>com.xpn.xwiki.wysiwyg.server.filter.MutableServletRequestFactory</role>
      <role-hint>HTTP/1.1</role-hint>
      <implementation>
        com.xpn.xwiki.wysiwyg.server.filter.http.MutableHttpServletRequestFactory
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>
