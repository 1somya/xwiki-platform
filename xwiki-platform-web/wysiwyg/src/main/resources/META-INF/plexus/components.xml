        com.xpn.xwiki.wysiwyg.server.converter.internal.FakeHTMLConverter
      </implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
    </component>
    <component>
      <role>com.xpn.xwiki.wysiwyg.server.converter.HTMLConverter</role>
      <role-hint>xwiki/2.0</role-hint>
