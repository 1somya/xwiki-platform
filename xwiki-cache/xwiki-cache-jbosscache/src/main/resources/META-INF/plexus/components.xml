<component-set>
  <components>
    <component>
      <role>org.xwiki.cache.CacheFactory</role>
      <role-hint>jbosscache</role-hint>
      <implementation>org.xwiki.cache.jbosscache.internal.JBossCacheCacheFactory</implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
      <lifecycle-handler>xwiki</lifecycle-handler>
      <requirements>
        <requirement>
          <role>org.xwiki.container.Container</role>
          <role-hint>default</role-hint>
        </requirement>
      </requirements>
    </component>
    <component>
      <role>org.xwiki.cache.CacheFactory</role>
      <role-hint>jbosscache/local</role-hint>
      <implementation>org.xwiki.cache.jbosscache.internal.JBossCacheLocalCacheFactory</implementation>
      <instantiation-strategy>singleton</instantiation-strategy>
      <lifecycle-handler>xwiki</lifecycle-handler>
      <requirements>
        <requirement>
          <role>org.xwiki.container.Container</role>
          <role-hint>default</role-hint>
        </requirement>
      </requirements>
    </component>
  </components>
</component-set>
