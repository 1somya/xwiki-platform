#template("xwiki.vm")
#template("httpheader.vm")
#template("header.vm")
#macro( versions )
#foreach( $rev in $doc.recentRevisions )
<a href="$viewurl?rev=$rev">$rev</a><span> | </span>
#end
#end
<div id="xwikinav_header">
<div class="xwikiactions">
<a href="$editurl">$msg.get("edit")</a><span> | </span>
<a href="$inlineurl">$msg.get("inline")</a><span> | </span>
<a href="$attachurl">$msg.get("attach")</a><span> | </span>
<a href="${viewurl}?raw=1">$msg.get("raw")</a><span> | </span>
#versions()
</div>
<div id="xwikiwebmenu">
$!xwiki.getWebPreference("menu")
</div>
</div>
#if($message)
<div id="xwikimessage">
$message
</div>
#end
<div id="xwikicontent">
#if ($request.getParameter("raw").equals("1"))
<pre>
$cdoc.getEscapedContent()
</pre>
#else
$cdoc.getRenderedContent()
#end
</div>
<div id="xwikinav_footer">
<div class="xwikiactions">
<a href="$editurl">$msg.get("edit")</a>
<a href="$inlineurl">$msg.get("inline")</a><span> | </span>
<a href="$attachurl">$msg.get("attach")</a><span> | </span>
<a href="${viewurl}?raw=1">$msg.get("raw")</a><span> | </span>
#versions()
</div>
</div>
#template("footer.vm")
