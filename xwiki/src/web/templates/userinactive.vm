#template("xwiki.vm")
#template("httpheader.vm")
#template("header.vm")
<div id="xwikinav_header">
<div id="xwikiwebusermenu">
<span id="xwikiwebusermenutitle">$msg.get("webusermenu")</span>
$!xwiki.getWebPreference("menu")
</div>
</div>
<div id="xwikicontent">
#if ($cause == "disabled")
$msg.get("accountdisabled")
#else
$msg.get("accountnotactive")
<br />
$msg.get("accountnotactive_email")
<br />
<form action="../../view/XWiki/RegisterNewUserConfirmation" method="get">
<input type="hidden" name="xwikiname" value="$context.user" />
<input type="text" name="validkey" size="16" />
<input type="submit" value="$msg.get("confirmaccount")" />
</form>
#end

</div>
<div id="xwikinav_footer">
</div>
#template("footer.vm")
