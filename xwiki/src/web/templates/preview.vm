#template("xwiki.vm")
#template("httpheader.vm")
#template("header.vm")
<div id="xwikinav_header">
<div class="xwikiactions">
<a href="$doc.getSaveUrl($context)" onclick="document.forms.edit.action=this.href; document.forms.edit.submit(); return false;" >$msg.get("save")</a>
</div>
</div>
<div id="xwikicontent">
$doc.getRenderedContent($context)
<form id="edit" method="post" action="$doc.getPreviewUrl($context)">
<p>
<input type="hidden" name="content" value="$xwiki.getFormEncoded($doc.content)" />
<input type="hidden" name="parent" value="$!doc.getParent()" />
<input type="hidden" name="template" value="$!doc.getTemplate()" />
</p>
<div id="xwikiobject">
#template("hiddenobject.vm")
</div>
</form>
</div>
<div id="xwikinav_footer">
<div class="xwikiactions">
<a href="$doc.getSaveUrl($context)" onclick="document.forms.edit.action=this.href; document.forms.edit.submit(); return false;" >$msg.get("save")</a>
</div>
</div>
#template("footer.vm")
