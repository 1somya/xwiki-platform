<div id="xwikibanner">
<div id="xwikilogo">
    <a href="../../view/Main/WebHome">
      <img src="$xwiki.getSkinFile("logo.gif")" alt="XPertNet" width="150" height="40" />
    </a>
  </div>

<div id="xwikititle">
#if ($doc.parent.length() > 0)
<h1>$parent &gt; $!headertitle</h1>
#else
<h1>$!headertitle</h1>
#end
   <h2>$!headersubtitle</h2>
</div>

<div id="xwikilinks">
  <div id="xwikimenu">
    $!xwiki.getXWikiPreference("menu")
  </div>
  <div id="xwikiversion">
    <a href="../../view/Main/WebHome">
#set( $version = $!xwiki.getWebPreference("version"))
#if ( $version != "")
$xwiki.parseContent($version)
#else
XWiki Version $xwiki.version
#end
    </a>
  </div>
</div>


</div>