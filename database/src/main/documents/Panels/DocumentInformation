&lt;div&gt;&lt;label for="xwikidocparentinput2"&gt;$msg.get("parentfield"):&lt;/label&gt; &lt;input type="text" id="xwikidocparentinput2" name="parent" value="$!tdoc.parent" size="30"/&gt;&lt;/div&gt;
&lt;div class="centered"&gt;&lt;input type="submit" class="button" value="$msg.get("set")"/&gt;&lt;/div&gt;
&lt;/div&gt;&lt;/form&gt;
