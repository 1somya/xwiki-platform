&lt;script type="text/javascript"&gt;
function gotoclass(dest) {
 if (dest!="---") {
   location = dest;
   }
   return false;
}
&lt;/script&gt;
&lt;input class="button" type="submit" value="$msg.get("changeclass")" onclick="return gotoclass(this.form.classname.value)" /&gt;&lt;/div&gt;
