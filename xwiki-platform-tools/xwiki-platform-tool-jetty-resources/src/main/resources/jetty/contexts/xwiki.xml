<?xml version="1.0"  encoding="ISO-8859-1"?>
<!DOCTYPE Configure PUBLIC "-//Mort Bay Consulting//DTD Configure//EN" "http://jetty.mortbay.org/configure.dtd">
<Configure class="org.mortbay.jetty.webapp.WebAppContext">
  <Set name="contextPath">/xwiki</Set>
  <Set name="war"><SystemProperty name="jetty.home" default="."/>/../webapps/xwiki</Set>
  <Set name="systemClasses">
    <Array type="java.lang.String">
      <Item>java.</Item>
      <Item>javax.servlet.</Item>
      <Item>javax.xml.</Item>
      <Item>org.xml.</Item>
      <Item>org.w3c.</Item>
      <Item>org.apache.commons.logging.</Item>
      <Item>org.apache.log4j.</Item>
      <Item>org.mortbay.jetty.servlet.</Item>
      <Item>org.mortbay.util.ajax.</Item>
      <Item>org.mortbay.naming.</Item>
      <Item>javax.script.</Item>
    </Array>
  </Set>
</Configure>